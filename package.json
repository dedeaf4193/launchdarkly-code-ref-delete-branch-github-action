{
  "name": "launchdarkly-delete-branch-action",
  "version": "1.0.0",
  "description": "GitHub Action to delete LaunchDarkly Code Refs branches",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build src/index.ts -o dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "github-action",
    "launchdarkly",
    "code-refs",
    "branch",
    "delete"
  ],
  "author": "LaunchDarkly",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nock": "^14.0.10",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
